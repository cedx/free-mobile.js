{
	"bugs": "https://github.com/cedx/free-mobile.js/issues",
	"description": "Send SMS messages to your Free Mobile device.",
	"homepage": "https://docs.belin.io/free-mobile.js",
	"license": "MIT",
	"name": "@cedx/free-mobile",
	"repository": "github:cedx/free-mobile.js",
	"type": "module",
	"version": "15.0.1",
	"author": {
		"email": "cedric@belin.io",
		"name": "CÃ©dric Belin",
		"url": "https://belin.io"
	},
	"devDependencies": {
		"@types/node": "^20.5.9",
		"c8": "^8.0.1",
		"eslint": "^8.48.0",
		"typedoc": "^0.25.1",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"exports": {
		"types": "./lib/index.d.ts",
		"import": "./src/index.js"
	},
	"files": [
		"lib/",
		"src/"
	],
	"imports": {
		"#free_mobile": {
			"types": "./lib/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"keywords": [
		"api",
		"client",
		"free",
		"mobile",
		"sms"
	],
	"scripts": {
		"build": "tsc --project src/jsconfig.json",
		"clean": "node scripts/clean.js",
		"doc": "typedoc --options etc/typedoc.json && node scripts/doc.js",
		"dist": "npm run clean && npm run build",
		"lint": "tsc --project jsconfig.json && eslint --config=etc/eslint.json example scripts src test",
		"postpublish": "node scripts/publish.js",
		"prepack": "npm run dist",
		"test": "c8 --all --include=src/**/*.js --report-dir=var --reporter=lcovonly node --test --test-reporter=spec"
	}
}
